{
	"name": "sveltekit-starter-prismic-minimal",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": "20.x"
	},
	"scripts": {
		"dev": "concurrently \"npm:vite:dev\" \"npm:slicemachine\" --prefix-colors blue,magenta",
		"vite:dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"slicemachine": "start-slicemachine"
	},
	"devDependencies": {
		"@iconify/svelte": "^4.2.0",
		"@slicemachine/adapter-sveltekit": "^0.3.68",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.15",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "^8.56.10",
		"@types/nodemailer": "^6.4.17",
		"@types/qrcode": "^1.5.5",
		"autoprefixer": "^10.4.20",
		"concurrently": "^8.2.2",
		"eslint": "^9.5.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.3",
		"globals": "^15.4.0",
		"postcss": "^8.5.2",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.4",
		"slice-machine-ui": "^2.13.1",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.6",
		"svelte2tsx": "^0.7.34",
		"tailwindcss": "^4.0.12",
		"tslib": "^2.6.3",
		"typescript": "^5.4.5",
		"typescript-eslint": "^8.0.0-alpha.20",
		"typescript-svelte-plugin": "^0.3.38",
		"vite": "^5.3.1"
	},
	"type": "module",
	"dependencies": {
		"@prismicio/client": "^7.16.0",
		"@prismicio/svelte": "^1.3.1",
		"@tailwindcss/vite": "^4.0.4",
		"@vercel/analytics": "^1.5.0",
		"@vercel/speed-insights": "^1.2.0",
		"dotenv": "^16.4.7",
		"nodemailer": "^6.10.0",
		"qrcode": "^1.5.4"
	}
}
